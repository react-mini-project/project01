doSlice.js",["461"],[],"/Users/JJY/Desktop/project01/src/redux/modules/commentsSlice.js",["462"],[],"/Users/JJY/Desktop/project01/src/Shared/Router.jsx",[],[],"/Users/JJY/Desktop/project01/src/Shared/GlobalStyle.jsx",[],[],"/Users/JJY/Desktop/project01/src/components/Button.jsx",["463"],[],"import React, { Children } from \"react\"\nimport styled from \"styled-components\"\n\nconst Button = ({width=\"100px\", borderColor=\"black\", bgColor=\"white\", color=\"black\", onClick, fontWeight=\"400\", children}) => {\n    return (\n        <ButtonCtn\n            width={width}\n            borderColor={borderColor}\n            bgColor={bgColor}\n            fontWeight = {fontWeight}\n            color = {color}\n            onClick = {onClick}\n        >\n           {children}\n        </ButtonCtn>\n    )\n}\n\nconst ButtonCtn = styled.button`\n    width: ${(props)=>props.width};\n    color: ${(props)=>props.color};\n    padding: 4px 10px;\n    border: 1px solid ${(props)=>props.borderColor};\n    border-radius: 10px;\n    background-color: ${(props)=>props.bgColor};\n    font-weight: ${(props)=>props.aaa};\n`\nexport default Button","/Users/JJY/Desktop/project01/src/pages/AddPage.jsx",[],[],"/Users/JJY/Desktop/project01/src/pages/Detail.jsx",[],[],"/Users/JJY/Desktop/project01/src/pages/Home.jsx",[],[],"/Users/JJY/Desktop/project01/src/pages/Todos.jsx",[],[],"/Users/JJY/Desktop/project01/src/components/layout/Layout.jsx",[],[],"/Users/JJY/Desktop/project01/src/components/addTodo/AddTodo.jsx",[],[],"/Users/JJY/Desktop/project01/src/components/commentsList/CommentsList.jsx",["464","465"],[],"import React, { useEffect, useState } from \"react\"\nimport styled from \"styled-components\"\nimport { useParams } from \"react-router-dom\"\nimport { useDispatch, useSelector } from \"react-redux\"\nimport useInput from \"../../hooks/useInput\"\nimport { __addComment, __getComments } from \"../../redux/modules/commentsSlice\"\nimport Comment from \"../comment/Comment\"\n\nconst CommentsList = () => {\n    const dispatch = useDispatch()\n    const {id} = useParams()\n    const todoId = id\n    \n    const { comments } = useSelector((state) => state.comments)\n    const thisComments = comments.filter((comment)=> {\n        return +comment.todoId === +id\n    })\n    \n    const [nickname, onChangeNicknameHandler, resetNickname] = useInput();\n    const [comment, onChangeCommentHandler, resetComment] = useInput();\n\n    const onSubmitAddComment = (e) => {\n        e.preventDefault();\n        dispatch(__addComment({todoId, nickname, comment}))\n        resetNickname()\n        resetComment()\n    }\n\n    \n    useEffect(() => {\n        dispatch(__getComments())\n    }, [])\n\n    return (\n        <CommentsListCtn>\n            <Footer>\n                <FooterTitle>\n                    댓글 보기\n                </FooterTitle>\n                <AddCommentForm onSubmit={onSubmitAddComment}>\n                    <AddCommentNickname\n                        type=\"text\"\n                        name=\"nickname\"\n                        value={nickname}\n                        onChange={onChangeNicknameHandler}\n                        placeholder=\"작성자의 이름을 입력해주세요. (5자 이내)\"\n                        maxLength=\"5\"\n                        required\n                    />\n                    <AddCommentContent\n                        type=\"text\"\n                        name=\"comment\"\n                        value={comment}\n                        onChange={onChangeCommentHandler}\n                        placeholder=\"내용을 입력해주세요. (100자 이내)\"\n                        maxLength=\"100\"\n                        required\n                    />\n                    <AddCommentBtn>추가하기</AddCommentBtn>\n                </AddCommentForm>\n                <CommentsCtn>\n                    {thisComments.slice(0).reverse().map((comment)=> (\n                        <Comment\n                            key={comment.id}\n                            comment={comment}\n                        />\n                    ))}\n                </CommentsCtn>\n\n                \n            </Footer>\n        </CommentsListCtn>\n    )\n}\nconst CommentsListCtn = styled.div``\nconst Footer = styled.div`\n    border-top: 1px solid #ddd;\n    background-color: white;\n    color: gray;\n    width: 100%;\n    font-size: 2em;\n    height: 1.1em;\n    /* height: 288px; */\n    position: fixed;\n    bottom: 0;\n    left: 0;\n    transition: all, 0.3s;\n    box-sizing: border-box;\n    box-shadow: 0 0 5px 1px rgba(0,0,0,0.2);\n    &:hover {\n        height: 288px;\n    }\n`\nconst FooterTitle = styled.div`\n    padding: 5px 20px;\n    font-size: 20px;\n`\nconst AddCommentForm = styled.form`\n    display: flex;\n    gap: 10px;\n    padding: 5px 20px;\n    border-top: 1px solid #ddd;\n    width: 100%;\n    padding-top: 10px;\n    color: #ddd;\n`\nconst AddCommentNickname = styled.input`\n    min-width: 100px;\n    width: 30%;\n    max-width: 300px;\n    border: 1px solid #ddd;\n    border-radius: 10px;\n    padding: 0 12px;\n    height: 46px;\n    font-size: 14px;\n`\nconst AddCommentContent = styled.input`\n    width: 70%;\n    border: 1px solid #ddd;\n    border-radius: 10px;\n    padding: 0 12px;\n    height: 46px;\n    font-size: 14px;\n`\nconst AddCommentBtn = styled.button`\n    background-color: transparent;\n    border: 2px solid #1a73e8;\n    color: #1a73e8;\n    font-weight: 700;\n    width: 100px;\n    height: 46px;\n    border-radius: 10px;\n    padding: 10px;\n    transition: all, 0.3s;\n    &:hover {\n        cursor: pointer;\n        background-color: #1a73e8;\n        color: white;\n    }\n`\nconst CommentsCtn = styled.div`\n    display: flex;\n    flex-direction: column;\n    padding: 5px 20px;\n    gap: 10px;\n    height: 190px;\n    overflow: auto;\n`\n\nexport default CommentsList","/Users/JJY/Desktop/project01/src/components/header/Header.jsx",[],[],"/Users/JJY/Desktop/project01/src/components/detailTodo/